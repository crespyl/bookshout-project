<h3>Search</h3>
<% if @user %>
    <p>
        Id: <%= @user.id %>
        <br/>
        User: <%= @user.name %>
        <br/>
        Email: <%= @user.email %>
        <p>
            <%= link_to "Log Out", logout_url %>
        </p>
   </p>
<% else %>
   <%= link_to "Log In", login_url %>
<% end %>
<p>
    Time: <%= Time.now %>
</p>
